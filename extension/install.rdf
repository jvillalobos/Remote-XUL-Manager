<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>remotexulmanager@xulforge.com</em:id>
    <em:name>Remote XUL Manager</em:name>
    <em:description>Manages the Remote XUL whitelist.</em:description>
    <em:version>1.3.1</em:version>
    <em:creator>Jorge Villalobos</em:creator>
    <em:homepageURL>http://github.com/jvillalobos/Remote-XUL-Manager</em:homepageURL>
    <em:optionsURL>chrome://remotexulmanager/content/rxmManager.xul</em:optionsURL>
    <em:bootstrap>true</em:bootstrap>
    <em:type>2</em:type>

    <!-- Mozilla Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>42.0</em:minVersion>
        <em:maxVersion>43.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- SeaMonkey -->
    <em:targetApplication>
      <Description>
        <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
        <em:minVersion>2.39</em:minVersion>
        <em:maxVersion>2.40.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Mozilla Firefox for Android -->
    <em:targetApplication>
      <Description>
        <em:id>{aa3c5121-dab2-40e2-81ca-7ea25febc110}</em:id>
        <em:minVersion>42.0</em:minVersion>
        <em:maxVersion>43.*</em:maxVersion>
      </Description>
    </em:targetApplication>

